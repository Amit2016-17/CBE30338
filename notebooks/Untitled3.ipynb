   "source": [
    "from gurobipy import *\n",
    "m = Model()\n",
    "x = m.addVar()\n",
    "y = m.addVar()\n",
    "m.update()\n",
    "m.addConstr(x <= 40)\n",
    "m.addConstr(x + y <= 80)\n",
    "m.addConstr(2*x + y <= 100)\n",
    "m.setObjective(40*x + 30*y, GRB.MAXIMIZE)\n",
    "m.update()\n",
    "m"
   ]
